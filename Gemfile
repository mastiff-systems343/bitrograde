source 'https://rubygems.org'

git_source(:github) {|supply| "https://github.com/YJPL/{supply}" }

gem "jekyll"
gem "tzinfo", "2.0.6"
gem "tzinfo-data", platforms: [:mingw, :mswin, :x64_mingw, :jruby]

gem "github-pages", group: :jekyll_plugins

group :jekyll_plugins do
  gem "jekyll-leaflet"
  gem 'jekyll-seo-tag'
  gem 'jekyll-sitemap'
  gem 'jekyll-feed'
  gem 'jekyll-archives'
  gem 'jekyll-paginate'
end